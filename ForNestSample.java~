public class ForNestSample
{
	public static void main(String[] args)
	{
        //System.out.println(0);
            
        //for(int row = 1; row <= 5; row++)
        int row = 0;
        while(row != 5)
       
        {
            System.out.print(row);


            for(int col = 0; col< 5;col++)
            {
                System.out.print(1);
            }


            System.out.println();
            row++;
        }
	}
}
