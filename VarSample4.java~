public class VarSample4
{
	public static void main(String[] args)
	{
		int x = 10;
		int y = 2;
		int[] ans = new int[3];

		ans[0] = x + y;
		System.out.println(ans[0]);

		ans[1] = x - y;
		System.out.println(ans[1]);

		ans[2] = x * y;
		System.out.println(ans[2]);

		ans[3] = x / y;
		System.out.println(ans[3]);
	}
}
