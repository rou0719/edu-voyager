public class SubstringSample
{
	public static void main(String[] args)
	{
        String s = args[0];
        int int1 = s.length();

		System.out.println("•¶Žš—ñ‚Ì’·‚³‚Í" + int1 + "‚Å‚·");

        for(int i = 0; i < int1; i++)
        {
            System.out.println(s.substring(i, i + 1));
        }     

	}
}
