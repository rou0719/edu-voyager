public class WhileBreak
{
	public static void main(String[] args)
	{
        int intCount = 0;
        int i = 0;

        while(i != 100)
        {
            System.out.println(i);
        
            if(i % 7 == 0)
            {
                intCount = intCount + 1;
                if(intCount == 8)
                {
                    break;
                }        
            }
            i++; 
        }
	}
}
