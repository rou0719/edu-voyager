public class SwitchSample2
{
    public static void main(String[] args)
    {
        char c = 'い';
      
        switch(c)
        {
            case 'あ':

                System.out.println("cの値は「あ」");
            
                break;

            case 'い':

                System.out.println("cの値は「い」");
            
                break;
                
            case 'う':

                System.out.println("cの値は「う」");
            
                break;

            case 'え':

                System.out.println("cの値は「え」");
            
                break;

            case 'お':

                System.out.println("cの値は「お」");
            
                break;
        }
    }
}    
